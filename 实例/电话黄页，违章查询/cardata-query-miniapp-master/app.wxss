/**app.wxss**/
page{
    color: #333;
    font-size: 32rpx;
    font-family: '微软雅黑';
}
.blackTip{
  z-index:2999;
  position:fixed;
  width:250rpx;
  background:#333;
  color:#fff;
  padding:36rpx;
  opacity:0.75;
  font-size:36rpx;
  top:30%;
  left:50%;
  border-radius:16rpx;
  text-align:center;
  word-break:break-all;
  -moz-transform:translateZ(0) translateX(-50%) translateY(-50%);
  -webkit-transform:translateZ(0) translateX(-50%) translateY(-50%);
  transform:translateZ(0) translateX(-50%) translateY(-50%);
}
.blackTip .info{
  width: 150rpx;
  height:150rpx;
  display: inline-block;
  background: url("data:image/png;base64,") no-repeat center;
  background-size: 80%;
}