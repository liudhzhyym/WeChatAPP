.TOT-view {
  height: 100%;
}

.list-view {
  padding: 18.75rpx 26.25rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}

.nav-view {
  width: 48%;
  height: 450rpx;
  margin-bottom: 26.25rpx;
  position: relative;
  background-color: rgb(237, 237, 237);
}

.nav-textview {
  position: absolute;
  width: 100%;
  bottom: 0rpx;
  background-color: #888;
  align-items: center;
  padding-top: 7rpx;
  padding-bottom: 7rpx;
}

.textview-text {
  font-size: 25rpx;
  color: #fff;
  padding-left: 10rpx;
  padding-right: 10rpx;
  align-items: center;
  text-align: center;
}

.nav-img {
  width: 100%;
  height: 100%;
  background-size: cover;
  background-position: center center;
  background-repeat: no-repeat;
}

.text-view {
  display: flex;
  justify-content: space-between;
}
